{% set assets_version=81 %}
<!DOCTYPE html>
<html lang="ru" >
<head>
  <meta charset="utf-8">
  <meta name="X-UA-Compatible" content="IE=edge">
  <meta name="language" content="ru" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
  <title>{{ global.header.title }}{% if global.server.REQUEST_URI!='/' and global.header.skip_last_title!=1%} | {{ global.fields.info.info_title }}{% endif %}</title>
  <meta property="og:title" content="{{ global.header.title }}" />
    {% if global.header.desc %}<meta property="og:description" content="{{ global.header.desc }}" />{% endif %}
    {% if global.header.desc %}<meta content="{{ global.header.desc }}" name="description">{% endif %}
    {% if global.header.keywords %}<meta content="{{ global.header.keywords }}" name="keywords">{% endif %}
    <meta name="cmsmagazine" content="23ba5200e2f0bd6a899aefc6f270df43" />
    <meta property="og:image" content="{% if global.header.og.image %}{{ global.header.og.image }}{% else %}{{ global.base_url }}/assets/images/static/og-image.jpg{% endif %}" />
    <meta property="og:url" content= "{{ global.base_url }}{{ global.server.REQUEST_URI }}" />
    <meta property="og:type" content="{% if global.header.og.type %}{{ global.header.og.type }}{% else %}website{% endif %}"/>
    {% block meta %}
      <link rel="canonical" href="{{ global.page_link_main }}"/>
      <meta name="robots" content="all"/>
    {% endblock %}
  <link rel="stylesheet" href="{{ global.base_url }}/assets/css/frontend.css?{{ assets_version }}">
  <link rel="shortcut icon" href="{{ global.base_url }}/favicon.ico" type="image/x-icon" />
  <link rel="apple-touch-icon" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon.png" />
  <link rel="apple-touch-icon" sizes="57x57" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-57x57.png" />
  <link rel="apple-touch-icon" sizes="72x72" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-72x72.png" />
  <link rel="apple-touch-icon" sizes="76x76" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-76x76.png" />
  <link rel="apple-touch-icon" sizes="114x114" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-114x114.png" />
  <link rel="apple-touch-icon" sizes="120x120" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-120x120.png" />
  <link rel="apple-touch-icon" sizes="144x144" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-144x144.png" />
  <link rel="apple-touch-icon" sizes="152x152" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-152x152.png" />
  <link rel="apple-touch-icon" sizes="180x180" href="{{ global.base_url }}/assets/images/icons/apple-touch-icon-180x180.png" />

  <script src="{{ global.base_url }}/assets/images/sprites/grunticon.loader.js"></script>
  <script>
      grunticon(["{{ global.base_url }}/assets/images/sprites/icons.data.svg.css", "{{ global.base_url }}/assets/images/sprites/icons.data.png.css", "{{ global.base_url }}/assets/images/sprites/icons.fallback.css"], grunticon.svgLoadedCallback);
  </script>
  <noscript><link href="{{ global.base_url }}/assets/images/sprites/icons.fallback.css" rel="stylesheet"></noscript>
  <!--[if lt IE 9]>
  <script src="{{ global.base_url }}/assets/components/html5shiv/dist/html5shiv.js"></script>
  <script src="{{ global.base_url }}/assets/components/respond/dest/respond.min.js"></script>
  <![endif]-->
</head>
{% set back_url=global.base_url %}
{% for item in global.page.breadcrumbs %}
  {% if item.link %}
    {% set back_url=item.link %}
  {% endif %}
{% endfor %}
<body {% if global.page.page_class %}class="page"{% endif %}>

{% block global_content %}{% endblock %}
<script type="text/javascript" src="{{ global.base_url }}/assets/js/lang/ru.js?{{ assets_version }}"></script>
<script type="text/javascript" src="{{ global.base_url }}/assets/js/frontend.min.js?{{ assets_version }}"></script>


<!-- Yandex.Metrika counter -->

<!-- /Yandex.Metrika counter -->
</body>
</html>
