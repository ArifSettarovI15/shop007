{% for item in list %}
    <tr>
        <td>
            {{ item.subscribe_id }}
        </td>
        <td>
            {{ item.subscribe_email }}
        </td>
        <td>{{ item.list_names }}</td>
        <td class="action_button_data">
            <a target="_blank" href="{{ global.base_url }}/manager/shop/subscribes/edit/{{ item.subscribe_email  }}/" class="btn btn-primary btn-xs"><i class="fa fa-pencil"></i></a>
            <div data-value="{{ item.subscribe_id }}" class="btn btn-danger btn-xs delete"><i class="fa"></i></div>
        </td>
    </tr>
{% endfor %}